    "openapi": "3.0.0",
    "info": {
        "description": "None",
        "version": "3.0.0",
        "title": "None"
    },
    "tags": [
        {
            "name": "general",
            "description": "General settings and information"
        }
    ],
    "paths": {
        "/expert/v3/snapshot/{instanceID}": {
            "post": {
                "tags": [
                    "general"
                ],
                "summary": "Restores a saved snapshot for specified instanceID",
                "operationId": "postSnapshot",
                "parameters": [
                    {
                        "name": "instanceID",
                        "in": "path",
                        "description": "instance identifier",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "maxLength": 32,
                            "pattern": "[A-Za-z0-9]+"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "snapshot": {
                                        "description": "Snapshot file to be restored",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "required": [
                                    "snapshot"
                                ]
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "$ref": "#/components/schemas/Error"
                    }
                }
            }
        }
    },
    "servers": [
        {
            "url": "http://localhost:5007"
        }
    ],
    "components": {
        "schemas": {
            "Error": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "uint16"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        }
    }
}